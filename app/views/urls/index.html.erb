<div class="row">

  <div class="col-2">
  </div>

  <div class="col-8">
    
    <div class="content">
      <h1>Enter a URL to be Shortened.</h1>

      <%= form_for Url.new, url: urls_path, method: :post do |f| %>
        <%= f.text_field :original_url, value: params[:original_url] %>
        <%= f.submit 'Submit' %>
      <% end %>

    </div>
    

  </div>

  <div class="col-2">
  </div>

</div>

